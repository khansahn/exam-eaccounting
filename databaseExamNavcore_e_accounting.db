CREATE TABLE "barang" (
  "id" serial PRIMARY KEY,
  "name" varchar NOT NULL,
  "jumlah" int,
  "hb" float,
  "hj" float,
  "satuan" varchar,
  "created_at" timestamp,
  "status_enabled" boolean
);

CREATE TABLE "transaksi" (
  "id" serial PRIMARY KEY,
  "name" varchar NOT NULL,
  "created_at" timestamp,
  "status_enabled" boolean,
  "id_barang" int,
  "jumlah" int,
  "tipe" varchar
);

CREATE TABLE "cashflow" (
  "id" serial PRIMARY KEY,
  "created_at" timestamp,
  "status_enabled" boolean,
  "cash_in" float,
  "cash_out" float,
  "balance" float,
  "id_transaksi" int
);

ALTER TABLE "transaksi" ADD FOREIGN KEY ("id_barang") REFERENCES "barang" ("id");

ALTER TABLE "cashflow" ADD FOREIGN KEY ("id_transaksi") REFERENCES "transaksi" ("id");